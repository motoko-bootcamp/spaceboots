type Boot = service {
  getOwner : () -> (principal) query;
  http_request : (Request) -> (Response) query;
  transfer : () -> () oneway;
};
type HeaderField = record { text; text };
type Method = text;
type Request = record {
  url : text;
  method : Method;
  body : vec nat8;
  headers : vec HeaderField;
};
type Response = record {
  body : vec nat8;
  headers : vec HeaderField;
  status_code : StatusCode;
};
type StatusCode = nat16;
service : Boot