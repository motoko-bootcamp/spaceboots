type StatusCode = nat16;
type Response = 
 record {
   body: blob;
   headers: vec HeaderField;
   status_code: StatusCode;
 };
type Request = 
 record {
   body: blob;
   headers: vec HeaderField;
   method: Method;
   url: text;
 };
type Method = text;
type HeaderField = 
 record {
   text;
   text;
 };
type Boot = 
 service {
   getOwner: () -> (principal) query;
   http_request: (Request) -> (Response) query;
   transfer: () -> () oneway;
 };
service : () -> Boot
